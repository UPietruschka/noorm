<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<generatorConfiguration xmlns="http://www.noorm.org/generator-configuration/1.0.0"
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xsi:schemaLocation="http://www.noorm.org/generator-configuration/1.0.0 http://www.noorm.org/xsd/generator-configuration-1.0.0.xsd">

    <beanPackageName>org.noorm.test.hr.beans</beanPackageName>
    <enumPackageName>org.noorm.test.hr.enums</enumPackageName>
    <servicePackageName>org.noorm.test.hr.services</servicePackageName>
    <enumTableFilterRegex>REGIONS|OPT_VARIANTS</enumTableFilterRegex>
    <packageFilterRegex>(.*SERVICE|.*SEARCH)</packageFilterRegex>
    <table2SequenceMapping>
        <property>
            <name>(EMPLOYEES|DEPARTMENTS|LOCATIONS|OPT_LOCK_.*|COMPLEX.*)</name>
            <value>$1_SEQ</value>
        </property>
        <property>
            <name>(case_sensitive_table)</name>
            <value>CASE_SENSITIVE_TABLE_SEQ</value>
        </property>
        <property>
            <name>(V_DEPARTMENTS)</name>
            <value>DEPARTMENTS_SEQ</value>
        </property>
    </table2SequenceMapping>
    <ignoreColumnNamePrefixes>
        <prefix>OPT_</prefix>
    </ignoreColumnNamePrefixes>
    <enumTable2DisplayColumnMapping>
        <property>
            <name>REGIONS</name>
            <value>REGION_NAME</value>
        </property>
        <property>
            <name>OPT_VARIANTS</name>
            <value>OPT_VARIANT_NAME</value>
        </property>
    </enumTable2DisplayColumnMapping>
    <optLockVersionColumnMapping>
        <property>
            <name>OPT_LOCK_.*</name>
            <value>OPT_VERSION</value>
        </property>
        <property>
            <name>COMPLEX_.*</name>
            <value>VERSION</value>
        </property>
        <property>
            <name>case_.*</name>
            <value>version</value>
        </property>
    </optLockVersionColumnMapping>
    <optLockFullRowCompareTableRegex>V_DEPARTMENTS|JOBS|JOB_HISTORY</optLockFullRowCompareTableRegex>
    <viewName2PrimaryKeyMapping>
        <property>
            <name>V_DEPARTMENTS</name>
            <value>DEPARTMENT_ID</value>
        </property>
    </viewName2PrimaryKeyMapping>
    <singleRowFinderRegex>(.*_unique.*)</singleRowFinderRegex>
    <queryDeclarations>
        <queryDeclaration>
            <tableName>EMPLOYEES</tableName>
            <queryColumns>
                <queryColumn>
                    <columnName>COMMISSION_PCT</columnName>
                    <operator>
                        <operatorName>EQUAL_TO</operatorName>
                    </operator>
                </queryColumn>
            </queryColumns>
        </queryDeclaration>
        <queryDeclaration>
            <tableName>EMPLOYEES</tableName>
            <methodName>findEmployeesWithCommission</methodName>
            <queryColumns>
                <queryColumn>
                    <columnName>COMMISSION_PCT</columnName>
                    <operator>
                        <operatorName>IS_NOT_NULL</operatorName>
                    </operator>
                </queryColumn>
            </queryColumns>
        </queryDeclaration>
        <queryDeclaration>
            <tableName>EMPLOYEES</tableName>
            <methodName>findEmployeesWithoutCommission</methodName>
            <queryColumns>
                <queryColumn>
                    <columnName>COMMISSION_PCT</columnName>
                    <operator>
                        <operatorName>IS_NULL</operatorName>
                    </operator>
                </queryColumn>
            </queryColumns>
        </queryDeclaration>
        <queryDeclaration>
            <tableName>DEPARTMENTS</tableName>
            <singleRowQuery>true</singleRowQuery>
            <queryColumns>
                <queryColumn>
                    <columnName>DEPARTMENT_ID</columnName>
                    <operator>
                        <operatorName>EQUAL_TO</operatorName>
                    </operator>
                </queryColumn>
            </queryColumns>
        </queryDeclaration>
        <queryDeclaration>
            <tableName>JOBS_SYNONYM</tableName>
            <methodName>findJobsSynonymsBySalary</methodName>
            <queryColumns>
                <queryColumn>
                    <columnName>MIN_SALARY</columnName>
                    <operator>
                        <operatorName>GREATER_THAN_OR_EQUAL_TO</operatorName>
                    </operator>
                </queryColumn>
                <queryColumn>
                    <columnName>MAX_SALARY</columnName>
                    <operator>
                        <operatorName>LESS_THAN_OR_EQUAL_TO</operatorName>
                    </operator>
                </queryColumn>
            </queryColumns>
        </queryDeclaration>
        <queryDeclaration>
            <tableName>COUNTRIES</tableName>
            <acquireLock>true</acquireLock>
            <queryColumns>
                <queryColumn>
                    <columnName>COUNTRY_ID</columnName>
                    <operator>
                        <operatorName>EQUAL_TO</operatorName>
                    </operator>
                </queryColumn>
            </queryColumns>
        </queryDeclaration>
        <queryDeclaration>
            <tableName>V_EMPLOYEES</tableName>
            <baseTable>EMPLOYEES</baseTable>
            <methodName>findEmployeesByDepartmentCity</methodName>
            <className>EmployeeFinder</className>
            <queryColumns>
                <queryColumn>
                    <columnName>DEPARTMENT_NAME</columnName>
                </queryColumn>
                <queryColumn>
                    <columnName>CITY</columnName>
                </queryColumn>
            </queryColumns>
        </queryDeclaration>
        <queryDeclaration>
            <tableName>V_EMPLOYEES</tableName>
            <baseTable>EMPLOYEES</baseTable>
            <className>EmployeeFinder</className>
            <queryColumns>
                <queryColumn>
                    <columnName>DEPARTMENT_NAME</columnName>
                    <operator>
                        <operatorName>LIKE</operatorName>
                    </operator>
                </queryColumn>
            </queryColumns>
        </queryDeclaration>
        <queryDeclaration>
            <tableName>case_sensitive_table</tableName>
        </queryDeclaration>
    </queryDeclarations>
    <customTypeMappings>
        <customTypeMapping>
            <tableFilterRegex>COMPLEX_DATA_TYPES</tableFilterRegex>
            <columnFilterRegex>CONVERTED_NUMBER</columnFilterRegex>
            <databaseType>NUMBER</databaseType>
            <javaType>String</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <columnFilterRegex>COMMISSION_PCT</columnFilterRegex>
            <databaseType>NUMBER</databaseType>
            <javaType>java.math.BigDecimal</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <columnFilterRegex>REGION_ID|DEPARTMENT_ID|OPT_ID|LOCATION_ID</columnFilterRegex>
            <databaseType>NUMBER</databaseType>
            <javaType>Integer</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <parameterFilterRegex>P_DEPARTMENT_ID</parameterFilterRegex>
            <databaseType>NUMBER</databaseType>
            <javaType>Integer</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <tableFilterRegex>COUNTRIES</tableFilterRegex>
            <databaseType>NUMBER</databaseType>
            <javaType>Integer</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <columnFilterRegex>BOOLEAN_FLAG</columnFilterRegex>
            <databaseType>NUMBER</databaseType>
            <javaType>Boolean</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <columnFilterRegex>CONVERTED_DATE</columnFilterRegex>
            <databaseType>DATE</databaseType>
            <javaType>java.sql.Date</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <columnFilterRegex>CONVERTED_TIMESTAMP</columnFilterRegex>
            <databaseType>TIMESTAMP</databaseType>
            <javaType>java.sql.Timestamp</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <columnFilterRegex>CONVERTED_CLOB_COLUMN</columnFilterRegex>
            <databaseType>CLOB</databaseType>
            <javaType>java.sql.Clob</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <columnFilterRegex>CONVERTED_NCLOB_COLUMN</columnFilterRegex>
            <databaseType>NCLOB</databaseType>
            <javaType>java.sql.NClob</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <columnFilterRegex>CONVERTED_BLOB_COLUMN</columnFilterRegex>
            <databaseType>BLOB</databaseType>
            <javaType>java.sql.Blob</javaType>
        </customTypeMapping>
        <customTypeMapping>
            <columnFilterRegex>CONVERTED_XMLTYPE</columnFilterRegex>
            <databaseType>XMLTYPE</databaseType>
            <javaType>java.sql.SQLXML</javaType>
        </customTypeMapping>
    </customTypeMappings>

</generatorConfiguration>
